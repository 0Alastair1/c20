{
    "ErrorDocument": {
        "Key" : "404/index.html"
    },
    "IndexDocument": {
        "Suffix": "index.html"
    },
    "RoutingRules": [
        {
            "Condition": {
                "KeyPrefixEquals": "tools"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/tools"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "h1/tools/h1a-ek/using-custom-content-paths"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/using-custom-content-paths"
            }
        }
    ]
}