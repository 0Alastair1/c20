{
    "ErrorDocument": {
        "Key" : "404/index.html"
    },
    "IndexDocument": {
        "Suffix": "index.html"
    },
    "RoutingRules": [
        {
            "Condition": {
                "KeyPrefixEquals": "tools"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/tools"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "h1/tools/h1a-ek/using-custom-content-paths"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/using-custom-content-paths"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "h1/engine/build-types"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/engine/build-types"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "h1/source-data/animation-data"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/source-data/animation-data"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "h2/sources"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/source-data"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "h1/tools/h1a-ek/using-fbx-in-h1a"
            },
            "Redirect": {
                "ReplaceKeyPrefixWith": "general/source-data/fbx"
            }
        },
        {
            "Condition": {
                "KeyPrefixEquals": "tools/jointed-model-blender-toolset"
            },
            "Redirect" : {
                "ReplaceKeyPrefixWith": "general/tools/halo-asset-blender-development-toolset"
            }
        }
    ]
}